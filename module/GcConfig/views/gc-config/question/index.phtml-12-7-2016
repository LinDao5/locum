 /** @var $this \Zend\View\Renderer\PhpRenderer */
$paginatorEmpDen = new \Zend\Paginator\Paginator(new \Zend\Paginator\Adapter\ArrayAdapter($this->questionsEmpDen));
$paginatorEmpDen->setItemCountPerPage(10);
$paginatorEmpDen->setCurrentPageNumber(empty($_GET['page']) ? 1 : (int)$_GET['page']);

$paginatorEmpOpt = new \Zend\Paginator\Paginator(new \Zend\Paginator\Adapter\ArrayAdapter($this->questionsEmpOpt));
$paginatorEmpOpt->setItemCountPerPage(10);
$paginatorEmpOpt->setCurrentPageNumber(empty($_GET['page']) ? 1 : (int)$_GET['page']);

$paginatorEmpPha = new \Zend\Paginator\Paginator(new \Zend\Paginator\Adapter\ArrayAdapter($this->questionsEmpPha));
$paginatorEmpPha->setItemCountPerPage(10);
$paginatorEmpPha->setCurrentPageNumber(empty($_GET['page']) ? 1 : (int)$_GET['page']);


$paginatorFreDen = new \Zend\Paginator\Paginator(new \Zend\Paginator\Adapter\ArrayAdapter($this->questionsFreDen));
$paginatorFreDen->setItemCountPerPage(10);
$paginatorFreDen->setCurrentPageNumber(empty($_GET['page']) ? 1 : (int)$_GET['page']);

$paginatorFreOpt = new \Zend\Paginator\Paginator(new \Zend\Paginator\Adapter\ArrayAdapter($this->questionsFreOpt));
$paginatorFreOpt->setItemCountPerPage(10);
$paginatorFreOpt->setCurrentPageNumber(empty($_GET['page']) ? 1 : (int)$_GET['page']);

$paginatorFrePha = new \Zend\Paginator\Paginator(new \Zend\Paginator\Adapter\ArrayAdapter($this->questionsFrePha));
